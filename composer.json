{
  "name": "oacc/slim-jwt-auth",
  "description": "Blog using slim, doctrine, twig",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Sean Cooper",
      "email": "sean@orderandchaoscreative.com",
      "homepage": "https://orderandchaoscreative.com/"
    }
  ],
  "require": {
    "slim/slim": "^3.0",
    "monolog/monolog": "^1.17",
    "doctrine/orm": "^2.5",
    "lcobucci/jwt": "^3.2",
    "kanellov/slim-twig-flash": "^0.2.0",
    "league/fractal": "^0.17.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Oacc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpunit"
  }
}
